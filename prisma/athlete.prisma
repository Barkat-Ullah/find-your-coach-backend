model Athlete {
    id          String   @id @default(auto()) @map("_id") @db.ObjectId
    fullName    String
    email       String   @unique
    profile     String?
    phoneNumber String?
    category    String[]
    address     String?

    createdAt   DateTime    @default(now())
    updatedAt   DateTime    @updatedAt
    user        User        @relation(fields: [email], references: [email])
    report Report[]
    favorite Favorite[]
    booking Booking[]
    review Review[]

    @@map("athletes")
}
