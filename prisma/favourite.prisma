model Favorite {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  athleteEmail String
  coachEmail   String
  isFavorite   Boolean @default(false)

  // athlete Athlete @relation(fields: [athleteEmail], references: [email])

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@unique([athleteEmail, coachEmail])
  @@map("favorites")
}
