model Admin {
    id            String   @id @default(auto()) @map("_id") @db.ObjectId
    fullName      String
    email         String   @unique
    profile       String?
    contactNumber String
    createdAt     DateTime @default(now())
    updatedAt     DateTime @updatedAt
    user          User     @relation(fields: [email], references: [email])

    @@map("admins")
}
